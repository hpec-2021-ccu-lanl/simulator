+std::string resources = resources_value.GetString();
+try { message->machines = IntervalSet::from_string_hyphen(resources, " ", "-"); }
+catch(const std::exception & e) { throw std::runtime_error(std::string("Invalid JSON message: ") + e.what());}
+send_message_at_time(timestamp,"server",IPMessageType::SCHED_RESOURCES_AVAILABLE,static_cast<void *>(message));
+
+}
