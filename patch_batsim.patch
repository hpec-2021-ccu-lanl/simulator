+ * Get resources
+ * ***********************/
+
+auto * message = new ResourcesAvailableMessage;
+xbt_assert(data_object.IsObject(),"Invalid JSON message: in event %d (SCHED_RESOURCES_AVAILABLE) data portion should be an object ",event_number);
+xbt_assert(data_object.HasMember("resources"),"Invalid JSON message: in event %d (SCHED_RESOURCES_AVAILABLE) there should be a resources member in data",event_number);
